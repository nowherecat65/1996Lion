campaignTrail_temp.cyoa = true
cyoAdventure = function(a) {
 ans = campaignTrail_temp.player_answers[campaignTrail_temp.player_answers.length - 1]
    if(ans == 3397){
        campaignTrail_temp.candidate_json[1]["fields"].first_name = "Republicans";
        campaignTrail_temp.candidate_json[1]["fields"].last_name = "Republicans";
    }
    if(ans == 3399){
        campaignTrail_temp.candidate_json[1]["fields"].first_name = "Republicans";
        campaignTrail_temp.candidate_json[1]["fields"].last_name = "Republicans";
    }
}

